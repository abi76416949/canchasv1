<h1>Registrar Nuevo Cliente</h1>
<%= form_with(model: @cliente, local: true) do |form| %>
  <div>
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>
  <div>
    <%= form.label :telefono %>
    <%= form.text_field :telefono %>
  </div>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.submit "Registrar" %>
  </div>
<% end %>
